{
  "name": "hono-openapi",
  "description": "OpenAPI schema generator for Hono",
  "version": "0.1.5",
  "license": "MIT",
  "keywords": ["hono", "openapi"],
  "homepage": "https://github.com/rhinobase/hono-openapi",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rhinobase/hono-openapi.git",
    "directory": "packages/ui"
  },
  "bugs": {
    "url": "https://github.com/rhinobase/hono-openapi/issues"
  },
  "peerDependencies": {
    "hono": "^4.6.4",
    "@hono/zod-validator": "^0.4.1",
    "zod": "^3.23.8",
    "zod-openapi": "^4.0.0",
    "openapi-types": "^12.1.3",
    "@valibot/to-json-schema": "^1.0.0-beta.3",
    "@hono/valibot-validator": "^0.5.1",
    "@openapi-contrib/json-schema-to-openapi-schema": "^3.0.3"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4"
  },
  "exports": {
    ".": {
      "import": "./index.esm.js",
      "require": "./index.cjs.js",
      "types": "./index.cjs.d.ts"
    },
    "./zod": {
      "import": "./zod.esm.js",
      "require": "./zod.cjs.js",
      "types": "./zod.cjs.d.ts"
    }
  }
}
